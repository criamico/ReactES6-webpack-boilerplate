{
  "name": "ReactES6-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate to configure basic react project with webpack, babel and sass. ",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "./node_modules/webpack/bin/webpack.js -p  --config webpack_prod.config.js",
    "clean-build": "npm run clean && ./node_modules/webpack/bin/webpack.js -p  --config webpack_prod.config.js",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --progress --colors --inline",
    "lint": "esw webpack.config.* src tools --color",
    "test": "babel-node spec/support/run.js"
  },
  "author": "criamico",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/criamico/ReactES6-webpack-boilerplate"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.1.0",
    "jasmine-enzyme": "^6.0.2",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.6.0",
    "npm-check": "^5.7.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-uglify-js-plugin": "^1.1.9"
  }
}
